{
  "name": "@foreningsregister/api",
  "version": "1.0.0",
  "description": "API för svenska föreningsregister",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@foreningsregister/types": "workspace:*",
    "@foreningsregister/adapters": "workspace:*",
    "@hono/zod-openapi": "^0.12.0",
    "@hono/node-server": "^1.8.0",
    "hono": "^4.0.0",
    "zod": "^3.22.0",
    "openapi-fetch": "^0.9.0",
    "dotenv": "^16.4.0",
    "@prisma/client": "^5.8.0",
    "prisma": "^5.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
